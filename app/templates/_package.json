{
    "name": "<%= projectName %>",
    "version": "0.0.1",
    "dependencies": {
        "badger-accordion": "^1.2.3",
        "choices.js": "^9.0.1",
        "foundation-sites": "^6.5.3",
        "hamburgers": "^1.1.3",
        "micromodal": "^0.4.6",
        "lodash": "^4.17.14",<% if (cssReset === 'normalize') { %>
        "normalize.css": "^8.0.1",<% } %>
        "object-fit-images": "^3.2.4",<% if (cssReset === 'ericmeyer') { %>
        "reset-css": "^4.0.1"<% } %>
    },
    "devDependencies": {
        "babelify": "~10.0.0",
        "@babel/core": "~7.4.5",
        "@babel/preset-env": "~7.4.5",
        "@babel/register": "~7.4.4",
        "babel-plugin-add-module-exports": "~1.0.2",
        "babel-eslint": "~10.0.1",
        "ansi-colors": "~3.2.4",
        "fancy-log": "~1.3.3",
        "foldero": "~0.1.1",
        "minimist": "~1.2.0",
        "rimraf": "~2.6.3",
        "gulp-plumber": "~1.2.1",
        "gulp-changed": "~4.0.0",
        "gulp-cheerio": "^0.6.3",
        "gulp-notify": "~3.2.0",
        "gulp-rename": "~1.4.0",
        "gulp-replace": "^1.0.0",
        "gulp-terser": "^1.2.0",
        "del": "~4.1.1",
        "gulp-if": "~2.0.2",
        "glob": "~7.1.4",
        "gulp-data": "~1.3.1",
        "gulp-sourcemaps": "~2.6.5",
        "gulp-svg-sprite": "^1.5.0",
        "gulp-svgmin": "^2.2.0",
        "gulp-load-plugins": "~1.6.0",
        "gulp-cssnano": "~2.1.3",
        "vinyl-source-stream": "~2.0.0",
        "vinyl-buffer": "~1.0.1",
        "what-input": "~5.2.3",
        "gulp": "~4.0.2",
        "browser-sync": "~2.26.7",<% if (htmlOption === 'pug') { %>
        "gulp-pug": "~4.0.1",
        "pug": "~2.0.3",<% } %><% if (htmlOption === 'nunjucks') { %>
        "nunjucks": "~3.2.0",
        "gulp-nunjucks-html": "~2.0.0",<% } %>
        "gulp-postcss": "~8.0.0",
        "autoprefixer": "~9.6.0",
        "gulp-htmlmin": "~5.0.1",
        "envify": "~4.1.0",
        "gulp-imagemin": "~6.0.0",
        "imagemin-svgo": "~7.0.0",
        "imagemin-pngquant": "~8.0.0",
        "gulp-eslint": "~5.0.0",<% if (cssOption === 'sass') { %>
        "gulp-sass": "~4.0.2",<% } %><% if (cssOption === 'stylus') { %>
        "gulp-stylus": "~2.7.0",<% } %>
        "js-yaml": "~3.13.1",
        "watchify": "~3.11.1 ",
        "browserify" : "~16.2.3"<% if (testFramework !== 'none') { %>,
        "karma": "~4.1.0",
        "karma-browserify": "~6.0.0",
        "karma-phantomjs-launcher": "~1.0.4"<% if (testFramework === 'jasmine') { %>,
        "karma-jasmine": "~2.0.1",
        "jasmine-core": "3.4.0"<% } else if (testFramework === 'mocha') { %>,
        "karma-chai": "~0.1.0",
        "karma-mocha": "~1.3.0",
        "mocha": "~6.1.4",
        "chai": "~4.2.0"<% } %><% } %>
    },
    "scripts": {
        "clean-deps": "rimraf node_modules",
        "clean": "gulp clean",
        "lint": "gulp eslint",
        "serve": "NODE_ENV=development gulp serve",
        "serve:prod": "NODE_ENV=production gulp serve --production",
        "build": "NODE_ENV=production gulp --production"
    },
    "engines": {
        "node": ">=0.12.0"
    },
    "//": "CUSTOM CONFIGURATION",
    "config": {
        "//": "Local Server Settings",
        "host": "127.0.0.1",
        "port": "3000",
        "baseUrl": "../",
        "//": "Gulp Task Directories",
        "//": "NOTE: folders prefixed with an underscore (_) will have it removed when moved to build target (ex: src/_images -> build/images)",
        "//": "NOTE: folders NOT prefixed with underscore (_) will be copied to build target 1 to 1 (ex: src/fonts -> build/fonts)",
        "directories": {
            "source": "src",
            "destination": "build",
            "temporary": "tmp",
            "//": "Directories relative to `source` directory",
            "modules": "_modules",
            "layouts": "_layouts",
            "images": "_images",
            "styles": "_styles",
            "scripts": "_scripts",
            "pages": "_pages",
            "data": "_data",
            "svgSprite": "svg/sprite",
            "svgIconSprite": "icons",
            "svgImgSprite": "imgs"
        },
        "//": "Entry files",
        "entries": {
            "js": "main**.js"<% if (cssOption === 'sass') { %>,
            "css": "main**.{sass,scss}"<% } else if (cssOption === 'stylus') { %>,
            "css": "main**.styl"<% } %> <% if (htmlOption === 'pug') { %>,
            "html": "**/*.pug"<% } else if (htmlOption === 'nunjucks') { %>,
            "html": "**/*.nunjucks"<% } %>,
            "image": "**/*.{jpg,jpeg,gif,svg,png}"
        }
    }
}
